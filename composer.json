{
    "name": "shiftonelabs/laravel-singular-resource-routes",
    "description": "Adds support for singular resource routes to the Laravel router.",
    "keywords": ["laravel", "singular", "resource", "routes"],
    "homepage": "https://github.com/shiftonelabs/laravel-singular-resource-routes",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Carlo-Hickman",
            "email": "patrick@shiftonelabs.com"
        }
    ],
    "support": {
        "issues": "https://github.com/shiftonelabs/laravel-singular-resource-routes/issues",
        "source": "https://github.com/shiftonelabs/laravel-singular-resource-routes"
    },
    "require": {
        "php": ">=5.5.0",
        "illuminate/routing": ">=5.0"
    },
    "require-dev": {
        "illuminate/events": ">=5.0",
        "laravel/framework": ">=5.0",
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": ">=4.0 <6.0",
        "shiftonelabs/codesniffer-standard": "0.*",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "ShiftOneLabs\\SingularResourceRoutes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ShiftOneLabs\\SingularResourceRoutes\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": false
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "ShiftOneLabs\\SingularResourceRoutes\\SingularResourceRoutesServiceProvider"
            ]
        }
    }
}
